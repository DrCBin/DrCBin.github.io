        <guid isPermaLink="true">http://drcbin.com/blog/%E8%BF%91%E6%9C%9F%E5%81%9A%E9%A1%B9%E7%9B%AE%E7%9A%84%E6%80%BB%E7%BB%93/</guid>
        
